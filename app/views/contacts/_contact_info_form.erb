<%= form_for ([user, contact]) do |f| %>
<div class="form-group row">
  <div class="col-sm-10">
    <%= f.text_field :name, placeholder: "Name" %>
  </div>
</div>

<div class="form-group row">

  <div class="col-sm-10">
    <%= f.text_field :email, placeholder: "Email" %>
  </div>
</div>
<div>
  <%= f.submit class: "btn btn-outline-primary inline"%>
</div>
<% end %>
